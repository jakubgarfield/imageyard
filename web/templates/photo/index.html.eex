<h2>Listing photos</h2>

<table class="table">
  <thead>
    <tr>
      <th>Set</th>
      <th>Name</th>
      <th>Container</th>
      <th>Dimensions</th>
      <th>Storage</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for photo <- @photos do %>
    <tr>
      <td><%= photo.set %></td>
      <td><%= photo.name %></td>
      <td><%= photo.container %></td>
      <td><%= photo.dimensions %></td>
      <td><%= photo.storage.name %></td>

      <td class="text-right">
        <%= link "Show", to: photo_path(@conn, :show, photo), class: "btn btn-default btn-xs" %>
        <%= link "Delete", to: photo_path(@conn, :delete, photo), method: :delete, class: "btn btn-danger btn-xs" %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<%= link "New photo", to: photo_path(@conn, :new) %>
